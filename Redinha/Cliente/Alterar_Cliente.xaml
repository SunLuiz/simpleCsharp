        <TextBox HorizontalAlignment="Left" Height="23" Margin="350,169,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="50"/>
        <Label x:Name="LbBairro" Content="Bairro" HorizontalAlignment="Left" Margin="33,216,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="76,216,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="167"/>
        <ComboBox HorizontalAlignment="Left" Margin="364,216,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Salvar _Alteração" HorizontalAlignment="Left" Height="41" Margin="63,306,0,0" VerticalAlignment="Top" Width="109"/>
        <Button Content="Nova Pequisa" HorizontalAlignment="Left" Height="41" Margin="214,306,0,0" VerticalAlignment="Top" Width="109"/>
        <Button Content="Fechar" HorizontalAlignment="Left" Height="41" Margin="364,306,0,0" VerticalAlignment="Top" Width="109" Click="Button_Click"/>

    </Grid>
</Window>

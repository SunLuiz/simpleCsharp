        <ComboBox x:Name="CBPesquisar" HorizontalAlignment="Left" Margin="271,51,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Nome"/>
            <ComboBoxItem Content="F.A."/>
        </ComboBox>
        <Label Content="Moradia" HorizontalAlignment="Left" Margin="291,216,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LbNum" Content="Numero" HorizontalAlignment="Left" Margin="291,165,0,0" VerticalAlignment="Top"/>
        <Label Content="Rua" HorizontalAlignment="Left" Margin="33,165,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LbFA" Content="F.A." HorizontalAlignment="Left" Margin="271,114,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbNome" Content="Nome" HorizontalAlignment="Left" Margin="20,110,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="76,114,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="172"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="305,117,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="66"/>
